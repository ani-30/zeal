<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>live</servlet-name>
        <servlet-class>live</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reregisteration</servlet-name>
        <servlet-class>Reregisteration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>email</servlet-name>
        <servlet-class>email</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Registeration</servlet-name>
        <servlet-class>Registeration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registeration</servlet-name>
        <url-pattern>/Registeration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>live</servlet-name>
        <url-pattern>/live</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reregisteration</servlet-name>
        <url-pattern>/Reregisteration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>email</servlet-name>
        <url-pattern>/email</url-pattern>
    </servlet-mapping>
    <error-page>
        <location>/error.jsp</location>
    </error-page>
</web-app>
